echo -e "$yy 99$y. SETTING$wh"
echo -e "$y-------------------------------------------------$wh"
echo -e ""
read -p "Select From Options [* - Keluar] : " menu
case $menu in
1)
clear
vmessmenu
;;
2)
clear
vlessmenu
;;
3)
clear
trmenu
;;
4)
clear
trgomenu
;;
97)
clear
ports
;;
98)
clear
serpices
;;
99)
clear
setting
;;
*)
clear
exit
;;
esac
